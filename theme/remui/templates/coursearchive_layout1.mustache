<div class="layout-1 container-fluid p-0">
  <div class="container p-4">
   	<div class="row">
   		<div class="col-12 col-md-12 col-lg-9 column left-column">
   		  {{> theme_remui/coursearchive_page_filters1}}

        <div class="course-cards row grid-view">
          <!-- Here we will render the cards from Javascript side. -->
        </div>

        <div class="w-100 cards-pagination">
            <!-- Here we will render the pagination from Javascript side. -->
        </div>

   		</div>
   		<div class="col-12 col-md-12 col-lg-3 column right-column">
          <div class="inner-container border-none px-4 py-3 mb-4 col-lg-12 showmycourses d-none d-lg-block">
            <div class="d-flex justify-content-between align-items-center">
              <div class="custom-control custom-switch " >
                <input type="checkbox" class="custom-control-input" id="switch-label1">
                <label class="custom-control-label" for="switch-label1"></label>
              </div>
              <span class="switch-label">{{#str}}showenrolledcourses, theme_remui {{/str}}</span>
            </div>
          </div>

	      {{> theme_remui/coursearchive_sec_categories}}

        {{> theme_remui/enrol_coursetags}}
   		</div>
   	</div>
  </div>
</div>

{{#js}}
require(['theme_remui/loader', 'theme_remui/coursecategorylayout1'], function(l, c){
  Breakpoints();

  c.init('{{{defaultcat}}}');
});
{{/js}}
